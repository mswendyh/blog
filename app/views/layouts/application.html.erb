<!DOCTYPE html>
<html>
  <head>
    <title>Wendy Hagenmaier</title>
      <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track' => true %>
      <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
      <script type="text/javascript" src="agency.js"></script>
      <script src="javascripts/cbpAnimatedHeader.js"></script>
      <%= javascript_include_tag 'cbpAnimatedHeader' %>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">


    <!-- Custom CSS -->
    <link href="/assets/stylesheets/agency.css" rel="stylesheet">

    <!-- Bootstrap Core CSS -->
    <link href="/assets/stylesheets/bootstrap.min.css" rel="stylesheet">



    <link href="//maxcdn.bootstrapcdn.com/font-awesome/4.2.0/css/font-awesome.min.css" rel="stylesheet">


    <!-- Custom Fonts -->
    <link href="/assets/font-awesome-4.1.0/css/font-awesome.min.css" rel="stylesheet" type="text/css">
    <link href="http://fonts.googleapis.com/css?family=Montserrat:400,700" rel="stylesheet" type="text/css">
    <link href='http://fonts.googleapis.com/css?family=Kaushan+Script' rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Droid+Serif:400,700,400italic,700italic' rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Roboto+Slab:400,100,300,700' rel='stylesheet' type='text/css'>



    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->

    <%= csrf_meta_tags %>
  </head>

  <body id="page-top" class="index">

    <!-- Navigation -->
    <nav class="navbar navbar-default navbar-fixed-top">
      <div class="container">
        <!-- Brand and toggle get grouped for better mobile display -->
        <div class="navbar-header page-scroll">
           <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
              <span class="sr-only">Toggle navigation</span>
              <span class="icon-bar"></span>
              <span class="icon-bar"></span>
              <span class="icon-bar"></span>
            </button>
              <a class="navbar-brand page-scroll" href="home#page-top">Archive All the Things</a>
        </div>

            <!-- Collect the nav links, forms, and other content for toggling -->
          <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
            <ul class="nav navbar-nav navbar-right">
              <li class="hidden">
                <a href="#page-top"></a>
              </li>
              <li>
                <a class="page-scroll" href="home#services">About</a>
              </li>
              <li>
                <a class="page-scroll" href="home#portfolio">Projects</a>
              </li>
              <li>
                <a class="page-scroll" href="home#about">Contact</a>
              </li>
              <li>
                <a class="page-scroll" href="home#team">Links</a>
              </li>
            </ul>
          </div>
            <!-- /.navbar-collapse -->
      </div>
        <!-- /.container-fluid -->
    </nav>

    <!-- Header -->
  <header>
    <div class="container">
      <div class="intro-text">
        <div class="intro-lead-in">Wendy Hagenmaier</div>
        <div class="intro-heading">Digital Archivist</div>
        <a href="#services" class="page-scroll btn btn-xl">Hack the Past; Design the Future</a>
      </div>
    </div>
  </header>


<%= yield %>
  
<%#=full_title(yield(:title))%>

    <!-- jQuery Version 1.11.0 -->
    <script src="javascripts/jquery-1.11.0.js"></script>
    <%#= javascript_include_tag "jquery-1.11.0.js" %>

    <!-- Bootstrap Core JavaScript -->
    <script src="javascripts/bootstrap.min.js"></script>
    <%#= javascript_include_tag "bootstrap.min.js" %>

    <!-- Plugin JavaScript -->
    <script src="http://cdnjs.cloudflare.com/ajax/libs/jquery-easing/1.3/jquery.easing.min.js"></script>
    <%#= javascript_include_tag "http://cdnjs.cloudflare.com/ajax/libs/jquery-easing/1.3/jquery.easing.min.js" %>

    <script src="javascripts/classie.js"></script>
    <%#= javascript_include_tag "classie.js" %>

    <script src="javascripts/cbpAnimatedHeader.js"></script>
    <%#= javascript_include_tag "cbpAnimatedHeader.js" %>

    <!-- Contact Form JavaScript -->
    <script src="javascripts/jqBootstrapValidation.js"></script>
    <%#= javascript_include_tag "jqBootstrapValidation.js" %>

    <script src="javascripts/contact_me.js"></script>
    <%#= javascript_include_tag "contact_me.js" %>


    <!-- Custom Theme JavaScript -->
    <script type="text/javascript" src="javascripts/agency.js"></script>
    <%#= javascript_include_tag "agency.js" %>



</body>
</html>
